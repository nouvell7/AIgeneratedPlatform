{
  "name": "ai-service-platform",
  "version": "1.0.0",
  "description": "AI 모델을 활용한 웹 애플리케이션 생성, 배포, 수익화 통합 플랫폼",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "npm run dev --workspace=packages/backend",
    "dev:frontend": "npm run dev --workspace=packages/frontend",
    "build": "npm run build --workspace=packages/backend && npm run build --workspace=packages/frontend",
    "build:backend": "npm run build --workspace=packages/backend",
    "build:frontend": "npm run build --workspace=packages/frontend",
    "start": "npm run start --workspace=packages/backend",
    "test": "npm run test --workspaces",
    "test:backend": "npm run test --workspace=packages/backend",
    "test:frontend": "npm run test --workspace=packages/frontend",
    "test:coverage": "npm run test:coverage --workspaces",
    "test:e2e": "npm run test:e2e --workspace=packages/frontend",
    "test:watch": "npm run test:watch --workspaces",
    "lint": "npm run lint --workspaces",
    "lint:fix": "npm run lint:fix --workspaces",
    "type-check": "npm run type-check --workspaces",
    "db:generate": "npm run db:generate --workspace=packages/backend",
    "db:push": "npm run db:push --workspace=packages/backend",
    "db:seed": "npm run db:seed --workspace=packages/backend",
    "deploy": "./scripts/deploy.sh production",
    "deploy:dev": "./scripts/deploy.sh development"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^10.0.0",
    "concurrently": "^8.2.2",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}